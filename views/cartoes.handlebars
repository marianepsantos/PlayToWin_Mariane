<h1>Cartões Cadastrados</h1>


<a href="/cartoes/novo">Cadastrar novo cartão</a>

<table border="1">
    <tr>
        <!-- table head -->
        <th>Número</th>
        <th>Nome</th>
    </tr>

    {{#each cartoes}}
    <tr>
        <td>{{this.numero}}</td>
        <td>{{this.nome}}</td>
    </tr>
    {{/each}}
    </table>